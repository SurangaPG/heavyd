{
  "name": "heavyd/drupal-project",
  "description": "Project template for Drupal 8 projects with Composer made to work with the basic heavyd setup.",
  "type": "project",
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "",
      "role": ""
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  ],
  "require": {
    "composer/composer": "~1.0",
    "composer/installers": "^1.2",
    "cweagans/composer-patches": "^1.6",
    "drupal-composer/drupal-scaffold": "^2.2",
    "drupal/console": "^1.0.1",
    "drupal/core": "<8.4",
    "drush/drush": "~8.0",
    "php": ">=5.5.9",
    "drupal/adminimal_theme": "^1.2",
    "drupal/paragraphs": "^1",
    "drupal/field_group": "^1",
    "drupal/google_analytics": "^2",
    "drupal/config_installer": "^1",
    "drupal/metatag" : "^1",
    "drupal/simple_sitemap" : "^2",
    "drupal/inline_entity_form" : "^1",
    "drupal/captcha" : "^1",
    "drupal/recaptcha" : "^1",
    "drupal/image_widget_crop": "^2.0",
    "drupal/pathauto": "^1.0",
    "drupal/linkit": "^4.3",
    "oomphinc/composer-installers-extender": "^1.1",
    "webmozart/path-util": "^2.3",
    "webflo/drupal-finder": "^1.0.0"
  },
  "require-dev": {
    "behat/mink": "~1.7",
    "behat/mink-goutte-driver": "~1.2",
    "behat/mink-selenium2-driver": "dev-master#721cbbaf37f71434f6acb604dc3a17a6a9be0c4c",
    "drupal/coder": "~8.2.7",
    "drupal/config_update": "1.*@dev",
    "drupal/drupal-extension": "~3.0",
    "jcalderonzumba/gastonjs": "^1.1@dev",
    "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
    "mikey179/vfsStream": "~1.2",
    "phpunit/phpunit": ">=4.8.28 <5",
    "symfony/css-selector": "~2.8"
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "7.0"
    }
  },
  "autoload": {
    "psr-4": {
      "Drupal\\DrupalProject\\": "tests/src",
      "DrupalProject\\": "src"
    }
  },
  "extra": {
    "installer-paths": {
      "web/core": [
        "type:drupal-core"
      ],
      "web/libraries/{$name}": [
        "type:drupal-library"
      ],
      "web/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "web/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "web/themes/generic/{$name}": [
        "type:drupal-generic-theme"
      ],
      "web/modules/generic/{$name}": [
        "type:drupal-generic-module"
      ],
      "web/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "drush/contrib/{$name}": [
        "type:drupal-drush"
      ]
    }
  }
}